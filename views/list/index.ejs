<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Wishlist</title>
	</head>
	<body>
		<%- include('../partials/nav.ejs') %>
		<h2>Wishlistings</h2>
        <ul style="list-style-type: none;">
			<% wishes.forEach(wish => { %>
			<li class="indexList"><a href="/wishlist/<%= wish._id %>"> <img src="<%= wish.img %>" alt="<%= wish.name %>" width="300" height="300"></a>
            <br> <%= wish.name %> <br>
            <%= wish.owner %>
            </li>
			<% }); %>
		</ul>
        <nav>
            <a href="/wishlist/new">Add to my List</a>
        </nav>
	</body>
</html>